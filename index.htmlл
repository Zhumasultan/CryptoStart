<script type="text/javascript" src="https://s3.tradingview.com/tv.js"></script>

<script>
  window.addEventListener("load", function () {
    const configs = [
      { id: "tradingview_btc", symbol: "BINANCE:BTCUSDT" },
      { id: "tradingview_eth", symbol: "BINANCE:ETHUSDT" },
      { id: "tradingview_sol", symbol: "BINANCE:SOLUSDT" },
      { id: "tradingview_bnb", symbol: "BINANCE:BNBUSDT" },
      { id: "tradingview_xrp", symbol: "BINANCE:XRPUSDT" },
      { id: "tradingview_ada", symbol: "BINANCE:ADAUSDT" }
    ];

    const waitForTradingView = () => {
      if (typeof TradingView === 'undefined') {
        setTimeout(waitForTradingView, 100);
        return;
      }

      configs.forEach(cfg => {
        new TradingView.widget({
          "container_id": cfg.id,
          "width": "100%",
          "height": "220",
          "symbol": cfg.symbol,
          "interval": "D",
          "timezone": "Etc/UTC",
          "theme": "dark",
          "style": "1",
          "locale": "en",
          "toolbar_bg": "#000",
          "hide_top_toolbar": true,
          "hide_legend": true
        });
      });
    };

    waitForTradingView();
  });
</script>
</script>
    </body>
</html>
